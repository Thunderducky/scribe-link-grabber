{
  "name": "bot-test",
  "version": "1.0.0",
  "main": "server.js",
  "license": "MIT",
  "scripts": {
    "start": "per-env",
    "start:development": "concurrently \"nodemon --ignore client/\" \"npm run client\"",
    "start:production": "node server.js",
